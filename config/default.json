{
    "name": "notifier",
    "port":3004,
    "perPageSize":2000,
    "logger":{
      "appenders": [
        {
          "type": "console"
        },
        {
          "type": "file",
          "filename": "notifier.log",
          "maxLogSize": 20480,
          "numBackups": 5
        }
      ],
      "replaceConsole": true,
